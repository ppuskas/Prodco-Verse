{
    "version": 2,
    "name": "prodco-verse-graph",
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.js"
        },
        {
            "src": "/(.*\\.(js|css|png|jpg|svg|ico|woff2?))",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.html"
        }
    ]
}